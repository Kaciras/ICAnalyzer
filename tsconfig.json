{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable",
			"WebWorker"
		],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"sourceMap": true,
		"resolveJsonModule": true,
		"strict": true,
		"allowImportingTsExtensions": true,
		"useUnknownInCatchVariables": false,
		"baseUrl": ".",
		"paths": {
			"squoosh/*": ["./deps/squoosh/*"]
		}
	}
}
