{
	"exclude": [
		"node_modules",
		"deps"
	],
	"compilerOptions": {
		"target": "ES2020",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable",
			"WebWorker"
		],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"newLine": "LF",
		"sourceMap": true,
		"strict": true,
		"useUnknownInCatchVariables": false,
		"esModuleInterop": true,
		"removeComments": true,
		"baseUrl": ".",
		"paths": {
			"squoosh/*": ["./deps/squoosh/*"]
		}
	}
}
